# Sfold directories
SFOLDDIR=@abs_top_builddir@
SFOLDBIN=$SFOLDDIR/bin
SFOLDLIB=$SFOLDDIR/lib
SFOLDPAR=$SFOLDDIR/param

# Sfold programs
OSTYPE=@host_os@
MACH=@host_cpu@
SFOLDEXE=$SFOLDBIN/sfold.$MACH.$OSTYPE
FINDFEEXE=$SFOLDBIN/findfe.$MACH.$OSTYPE
GETU0EXE=$SFOLDBIN/getu0.$MACH.$OSTYPE
SCLASSEXE=$SFOLDBIN/sclass.$MACH.$OSTYPE
BPROBEXE=$SFOLDBIN/bprob.$MACH.$OSTYPE
FINDDISTEXE=$SFOLDBIN/find.dist.$MACH.$OSTYPE

# External programs
PATH_AWK=@AWK@
PATH_GREP=@GREP@
PATH_PERL=@PERL@
PATH_R=@R@
